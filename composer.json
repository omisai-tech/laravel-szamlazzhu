{
    "name": "omisai/laravel-szamlazzhu",
    "description": "Sz치ml치zz.hu Sz치mla Agent API integration for Laravel",
    "keywords": [
        "omisai",
        "szamlazzhu",
        "szamla-agent",
        "api-agent",
        "laravel"
    ],
    "license": "MIT",
    "homepage": "https://github.com/omisai-tech/laravel-szamlazzhu",
    "authors": [
        {
            "name": "S치ndor Gergely Bornemisza",
            "email": "bornemisza@omisai.com",
            "homepage": "https://omisai.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.2",
        "guzzlehttp/guzzle": "^7.5",
        "illuminate/config": "^10|^11|^12",
        "illuminate/filesystem": "^10|^11|^12",
        "illuminate/log": "^10|^11|^12",
        "nesbot/carbon": "^2.62.1|^3.0"
    },
    "require-dev": {
        "laravel/pint": "^1.8",
        "nunomaduro/phpinsights": "^2.8",
        "orchestra/testbench": "^7.0|^8.0|^9.0|^10.0",
        "pestphp/pest": "^2.5.0|^3.0",
        "pestphp/pest-plugin-arch": "^2.2|^3.0",
        "pestphp/pest-plugin-laravel": "^2.0|^3.0",
        "phpstan/phpstan": "^1.10.13|^2.0",
        "symfony/var-dumper": "^6.2.8|^7.0"
    },
    "autoload": {
        "psr-4": {
            "Omisai\\Szamlazzhu\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Omisai\\Szamlazzhu\\Tests\\": "tests"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "preferred-install": "dist",
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Omisai\\Szamlazzhu\\SzamlaAgentServiceProvider"
            ]
        }
    },
    "scripts": {
        "post-autoload-dump": "@php ./vendor/bin/testbench package:discover --ansi",
        "lint": "pint",
        "test:lint": "pint --test",
        "test:analyse": "./vendor/bin/phpstan analyse src tests",
        "test:unit": "./vendor/bin/pest --colors=always",
        "test:coverage": "./vendor/bin/pest --coverage",
        "test:insights": "./vendor/bin/phpinsights",
        "test": [
            "@test:lint",
            "@test:analyse",
            "@test:unit"
        ],
        "storage:link": "ln -sf ./vendor/orchestra/testbench-core/laravel/storage/app/private ./storage"
    }
}